<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Allow most app data to be backed up to the cloud -->
        <include domain="file" />
        <include domain="external" />
    </cloud-backup>
    <device-transfer>
        <!-- Allow most app data to be transferred to a new device -->
        <include domain="file" />
        <include domain="external" />
    </device-transfer>
</data-extraction-rules>
